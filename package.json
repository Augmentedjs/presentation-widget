{
  "name": "presentation-widget",
  "version": "2.5.6",
  "description": "The Augmented.js Next - Presentation Application Module.",
  "main": "dist/presentation-widget.js",
  "scripts": {
    "dev": "webpack --mode development --module-bind js=babel-loader --progress",
    "build": "webpack --mode production --module-bind js=babel-loader --progress",
    "lint": "eslint ./src/presentation-widget.js",
    "test": "mocha --require babel-core/register --require test/helper.js -c test/*Spec.js",
    "start": "node index.js",
    "docs": "rm -rf jsdoc && jsdoc ./src -r -d ./jsdoc/ -R ./README.md -t ./node_modules/foodoc/template -c ./jsdoc.conf.json -P ./package.json",
    "readme": "./node_modules/documentation/bin/documentation.js readme ./src/index.js -f md --section=API",
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run clean && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Augmentedjs/presentation-widget.git"
  },
  "keywords": [
    "augmentedjs",
    "presentation"
  ],
  "author": "Bob Warren <jedi.master@augmentedjs.com> (http://www.augmentedjs.com)",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "documentation": "^13.0.2",
    "eslint": "^7.4.0",
    "eslint-config-kentcdodds": "^14.14.1",
    "foodoc": "0.0.9",
    "jsdoc": "^3.6.4",
    "jsdom": "^16.2.2",
    "mocha": "^8.0.1",
    "mocha-jsdom": "^2.0.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "next-core-utilities": "^2.9.0"
  },
  "bugs": {
    "url": "https://github.com/Augmentedjs/presentation-widget/issues"
  },
  "homepage": "http://www.augmentedjs.com"
}
